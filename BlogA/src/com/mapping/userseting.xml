<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.usersetingDao">
	<select id="usersetingSelect" parameterType="int" resultType="userseting">
	select userId,username,userHeadImg from userseting where userId = #{_parameter}
	</select>
	
	<insert id="usersetingInsert" parameterType="userseting">
	insert into userseting (userId,username,userHeadImg) values (#{userId},#{username},#{userHeadImg})
	</insert>
	
	<update id="headImgUpdate" parameterType="int">
	update userseting set userHeadImg = #{1} where userId = #{0}
	</update>
	
</mapper>